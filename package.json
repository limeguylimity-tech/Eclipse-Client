{
  "name": "eclipse-client",
  "version": "1.0.0",
  "description": "",
  "main": "electron.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "build": {
    "appId": "com.eclipse.client",
    "productName": "Eclipse Client",
    "files": [
      "dist/**/*",
      "electron.cjs",
      "preload.cjs",
      "package.json",
      "lib/**/*",
      "audio/**/*",
      "backgrounds/**/*"
    ],
    "win": {
      "target": "portable",
      "icon": "public/icon.ico"
    },
    "linux": {
      "target": "deb",
      "category": "Game"
    }
  },
  "dependencies": {
    "axios": "^1.13.5",
    "framer-motion": "^12.34.3",
    "lucide-react": "^0.575.0",
    "minecraft-launcher-core": "^3.18.2",
    "msmc": "^5.0.5",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^9.2.1",
    "electron": "^40.6.0",
    "electron-builder": "^26.8.1",
    "electron-icon-builder": "^2.0.1",
    "vite": "^7.3.1",
    "wait-on": "^9.0.4"
  }
}